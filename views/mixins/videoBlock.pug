//- default
mixin videoBlock(video={})
    .videoBlock
        a(href=routes.videoDetail(video.id))
            video.videoBlock__Thumbnail(src=video.videoFile,poster=video.poster, controls=true)
            h4.videoBlock__title=video.title
            p.videoBllock__decription=video.description
            h6.videoBlock__views=video.views
        //- h6.videoBlock__
